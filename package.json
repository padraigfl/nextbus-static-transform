{
  "name": "nextbus-schedule-builder",
  "version": "0.1.0",
  "description": "A script to pull schedule data so as to reduce risk of hitting API limits",
  "main": "index.js",
  "scripts": {
    "test": "eslint -c .eslintrc.js --quiet src/* test/*",
    "nextbus_api_reqs": "node src/apiCalls.js sf-muni routes",
    "nextbus_aggregated": "node src/buildAggregatedData.js sf-muni",
    "nextbus_geojsons": "node src/buildGeoJsons.js sf-muni routes",
    "nextbus_setup": "npm run nextbus_api_reqs && npm run nextbus_aggregated && npm run nextbus_geojsons"
  },
  "keywords": [
    "nextbus",
    "api",
    "schedule",
    "build"
  ],
  "author": "padraigf (https://github.com/padraigfl)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "sleep": "^5.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-mocha": "^4.12.1",
    "mocha": "^5.0.4"
  }
}
